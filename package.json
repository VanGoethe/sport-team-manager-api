{
  "name": "sport-team-manager",
  "version": "0.0.1",
  "description": "soccer team manager app",
  "main": "app.ts",
  "scripts": {
    "build-ts": "tsc",
    "debug": "npm run build-ts",
    "start": "set NODE_ENV=production&& tsc && nodemon run dist/src/app.js",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "watch-node": "nodemon dist/src/app.js",
    "watch-ts": "tsc -w",
    "dev": "ts-node-dev app.ts"
  },

  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/config": "^0.0.38",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.11",
    "@types/express-validator": "^3.0.0",
    "@types/gravatar": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^15.0.2",
    "@types/request": "^2.48.5",
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.7",
    "request": "^2.88.2"
  },
  "engines": {
    "node": "14.16.1"
  }
